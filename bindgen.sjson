inputs = [
	"input"
]

output_folder = "."

force_ada_case_types = true

import_lib = "libfontconfig.lib"

package_name = "fontconfig"

remove_type_prefix = "Fc"

remove_macro_prefix = "FC_"

remove_function_prefix = "Fc"

clang_include_paths = [
    "input/freetype",
    "input/freetype/freetype",
    "input/freetype/config",
    "input/freetype/internal",
    "input/freetype/internal/services"
]
